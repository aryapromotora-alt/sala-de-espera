{
  "version": 2,
  "builds": [
    {
      "src": "src/api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "sala-de-espera-main/dist" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/sala-de-espera-main/dist/$1"
    }
  ]
}
